# Admin Panel Security Configuration
# Copy this file to .env and set your secure credentials

# Admin credentials (REQUIRED - Change these!)
ADMIN_USERNAME=your_secure_username
ADMIN_PASSWORD=your_very_strong_password_here

# Environment
NODE_ENV=development

# EmailJS Configuration (for production)
EMAILJS_PUBLIC_KEY=your_emailjs_public_key
EMAILJS_SERVICE_ID=your_emailjs_service_id
EMAILJS_NEWSLETTER_TEMPLATE=your_newsletter_template_id
EMAILJS_CONTACT_TEMPLATE=your_contact_template_id

# Site Configuration
SITE_DOMAIN=https://www.hemangpandhi.com
NEWSLETTER_FROM_EMAIL=noreply@hemangpandhi.com

